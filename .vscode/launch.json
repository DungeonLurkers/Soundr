{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch and Debug Standalone Blazor WebAssembly App",
            "type": "blazorwasm",
            "browser": "edge",
            "request": "launch",
            "url": "https://localhost:5004",
            "env": {
                "ASPNETCORE_URLS": "https://localhost:5004;http://localhost:5005"
            },
            "cwd": "${workspaceFolder}/Source/Runners/PlayerWasm",
            "program": "${workspaceFolder}/Source/Runners/PlayerWasm/bin/Debug/net5.0/PlayerWasm.dll"
        }
    ]
}